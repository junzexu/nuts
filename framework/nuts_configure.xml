
<?xml version="1.0" encoding="UTF-8"?>
<NutConf>
    <NutServer>
        <ServerTransports>
            <Transport>
                <Name>TSocket</Name>
                <Protocal>tcp</Protocal>
                <Host>0.0.0.0</Host>
                <Port>80</Port>
            </Transport>
            <Transport>
                <Name>TSocket</Name>
                <Protocal>udp</Protocal>
